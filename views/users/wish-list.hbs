<body>


    <!-- Offcanvas Menu Begin -->
    <!-- Offcanvas Menu Begin -->
    <div class="offcanvas-menu-overlay"></div>
    <div class="offcanvas-menu-wrapper">
        <div class="offcanvas__close">+</div>
        <ul class="offcanvas__widget">
            <li><span class="icon_search search-switch"></span></li>
            <li><a href="/wish-list"><span class="icon_heart_alt"></span>
                </a></li>
            <li><a href="/cartpage"><span class="icon_bag_alt"></span>
                </a></li>
        </ul>
        <div class="offcanvas__logo">
            <a href="./index.html"><img src="/product-detail/img/logo.png" alt=""></a>
        </div>
        <div id="mobile-menu-wrap"></div>
        <div class="offcanvas__auth">
            {{#if user}}
            <a href="/profile">{{user.name}}</a>
            {{else}}
            <a href="/login">Lgin</a>
            <a href="/signup">Register</a>
            {{/if}}
        </div>
    </div>
    <!-- Offcanvas Menu End -->

    <!-- Header Section Begin -->
    <header class="header" style="height: 70px;">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xl-3 col-lg-2">
                    <div class="header__logo">
                        <a href="/"><img src="/product-detail/img/logo.png" style="width: 22%;" alt=""></a>
                    </div>
                </div>
                <div class="col-xl-6 col-lg-7">
                    <nav class="header__menu">
                        <ul>
                            <li><a href="/">Home</a></li>
                            <li><a href="/productpage">Products</a></li>
                            <li><a href="/productpage">Shop</a></li>
                            <li><a href="#">Pages</a>
                            <li class="active"><a href="/wish-list">Wishlist</a></li>
                            <li><a href="/cartpage">Cart</a></li>
                        </ul>
                    </nav>
                </div>
                <div class="col-lg-3">
                    <div class="header__right">
                        <div class="header__right__auth">
                            {{#if user}}
                            <a href="/profile">{{user.name}}</a>
                            {{else}}
                            <a href="/login">Login</a>
                            <a href="/signup">Register</a>
                            {{/if}}
                        </div>
                        <ul class="header__right__widget">
                            <li><span class="icon_search search-switch"></span></li>
                            <li><a href="/wish-list"><span class="icon_heart_alt"></span>


                                </a></li>
                            <li><a href="/cartpage"><span class="icon_bag_alt"></span>

                                </a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="canvas__open">
                <i class="fa fa-bars"></i>
            </div>
        </div>
    </header>
    <!-- Header Section End -->
    <!-- Shop Cart Section Begin -->
   
<!-- Product Section Begin -->
<section class="product spad">
    <div class="container">
        {{#if products}}
        <div class="row">
            <div class="col-lg-4 col-md-4">
                <div class="section-title">
                    <h4>Wishlist</h4>
                </div>
            </div>    
        </div>
        <div class="row property__gallery">
            {{#each products}}
            <div class="col-lg-3 col-md-4 col-sm-6 mix women">
                <div class="product__item">
                     <div class="product__item__pic set-bg" data-setbg="/photos/{{this.image}}" style="    background-size: 100%;">
                        <ul class="product__hover">
                             <li><a href="/photos/{{this.image}}" class="image-popup"><span class="arrow_expand"></span></a></li>
                            <li><a href="/removeFromWishList/{{this._id}}"><span class="icon_heart_alt" style="color: red;"></span></a></li>
                            <li><a onclick="wishListToCart('{{this._id}}')"><span class="icon_bag_alt"></span></a></li>
                        </ul>
                    </div>
                    <div class="product__item__text">
<h6><a href="/product/{{this._id}}">{{this.productname}}</a></h6>
                        <div class="rating">
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                        </div>
                        <div class="product__price"><i class="bi bi-currency-rupee"></i>{{this.prize}}</div>
                    </div>
                </div>
            </div>
            {{/each}}
            
        </div>
        {{else if noUser}}
        <h2>no</h2>
        {{else}}
        <img src="/images/oopsWishlist.jpg" alt="" style="    max-width: 65%;margin: auto;">
                    <div style="    display: flex;
    justify-content: center;"><a href="/" class="primary-btn">Shop Now</a></div>

        {{/if}}
    </div>
</section>
<!-- Product Section End -->

    <!-- Shop Cart Section End -->
<!-- Instagram Begin -->
